{
  "name": "@a-front-end-project/a-ui",
  "version": "1.0.0",
  "description": "React components library",
  "author": "Sunhaoxiang",
  "private": false,
  "type": "module",
  "module": "./dist/a-ui.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/a-ui.js",
      "types": "./dist/main.d.ts"
    },
    "./types/": "./dist/components/"
  },
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "homepage": "https://github.com/sunhaoxiang/a-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunhaoxiang/a-ui"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/style.css"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "lint": "eslint src --ext ts,tsx --max-warnings 5 --report-unused-disable-directives --fix --cache",
    "tsc": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run test:run && npm run lint && npm run build",
    "release": "release-it"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "async-validator": "^4.2.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-mdx-gfm": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-vite": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-transition-group": "^4.4.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "commitlint": "^17.6.6",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.10.3",
    "husky": "^8.0.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.11.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "sass": "^1.62.1",
    "storybook": "^7.0.20",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.3",
    "vite-plugin-libcss": "^1.1.1",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run lint"
    ]
  }
}
